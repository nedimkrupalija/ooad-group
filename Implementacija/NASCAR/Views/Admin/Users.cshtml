@model IEnumerable<NASCAR.Models.RegisteredUser> 
@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">User email</th>
            <th scope="col">Contact</th>
            <th scope="col">First name</th>
            <th scope="col">Last name</th>
           
        </tr>
    </thead>
    <tbody>


        @foreach (var item in Model)
        {
            <tr class="table-info">
                <th class="mx-auto" scope="row">@item.Email</th>
                <td class="mx-auto">@item.Contact</td>
                <td class="mx-auto">@item.FirstName</td>
                <td class="mx-auto">@item.LastName</td>
            </tr>
        }


    </tbody>
</table>

